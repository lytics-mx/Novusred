<odoo>
    <template id="lytics_base_template" name="Lytics Base Template">
        <div class="container">
            <header id="top" data-anchor="true" data-name="Header" data-extra-items-toggle-aria-label="Botón de elementos adicionales" class="o_header_standard o_top_fixed_element">
                <nav aria-label="Main" class="navbar navbar-expand-lg navbar-light o_colored_level o_cc d-none d-lg-block shadow-sm">
                    <div id="o_main_nav" class="o_main_nav container">
                        <a href="/" class="navbar-brand logo me-4">
                            <img src="/portal_web_lytics/static/src/img/logo.png" class="img img-fluid" width="95" height="40" alt="Lytics Portal" loading="lazy"/>
                        </a>
                        <ul role="menu" id="top_menu" class="nav navbar-nav top_menu me-auto">
                            <li role="presentation" class="nav-item">
                                <a role="menuitem" href="/" class="nav-link active">Inicio</a>
                            </li>
                            <li role="presentation" class="nav-item">
                                <a role="menuitem" href="/contactus" class="nav-link">Contáctenos</a>
                            </li>
                        </ul>
                        <ul class="navbar-nav align-items-center gap-2 flex-shrink-0 justify-content-end ps-3">
                            <li>
                                <a role="button" title="Búsqueda" href="#" class="btn rounded-circle p-1 lh-1 o_navlink_background text-reset">
                                    <i class="oi oi-search fa-stack lh-lg"></i>
                                </a>
                            </li>
                            <li>
                                <a href="tel:+1 555-555-5556" class="nav-link o_nav-link_secondary p-2">
                                    <i class="fa fa-1x fa-fw fa-phone me-1"></i>
                                    <span class="o_force_ltr"><small>+1 555-555-5556</small></span>
                                </a>
                            </li>
                            <li class="dropdown o_no_autohide_item">
                                <a href="#" role="button" data-bs-toggle="dropdown" class="dropdown-toggle nav-link border-0">
                                    <span>Mitchell Admin</span>
                                </a>
                                <div role="menu" class="dropdown-menu js_usermenu dropdown-menu-end">
                                    <a href="/odoo" role="menuitem" class="dropdown-item ps-3 d-none">
                                        <i class="fa fa-fw fa-th me-1 small text-primary text-primary-emphasis"></i> Aplicaciones
                                    </a>
                                    <a href="/my/home" role="menuitem" class="dropdown-item ps-3">
                                        <i class="fa fa-fw fa-id-card-o me-1 small text-primary text-primary-emphasis"></i> Mi cuenta
                                    </a>
                                    <div class="dropdown-divider"></div>
                                    <a role="menuitem" class="dropdown-item ps-3" href="/web/session/logout?redirect=/">
                                        <i class="fa fa-fw fa-sign-out me-1 small text-primary text-primary-emphasis"></i> Cerrar sesión
                                    </a>
                                </div>
                            </li>
                            <li>
                                <a href="/contactus" class="btn btn-primary btn_cta">Contáctenos</a>
                            </li>
                        </ul>
                    </div>
                </nav>
                <nav aria-label="Mobile" class="navbar navbar-light o_colored_level o_cc o_header_mobile d-block d-lg-none shadow-sm">
                    <div class="o_main_nav container flex-wrap justify-content-between">
                        <a href="/" class="navbar-brand logo">
                            <img src="/portal_web_lytics/static/src/img/logo.png" class="img img-fluid" width="95" height="40" alt="Lytics Portal" loading="lazy"/>
                        </a>
                        <ul class="o_header_mobile_buttons_wrap navbar-nav flex-row align-items-center gap-2 mb-0">
                            <li>
                                <button class="nav-link btn me-auto p-2" type="button" data-bs-toggle="offcanvas" data-bs-target="#top_menu_collapse_mobile" aria-controls="top_menu_collapse_mobile" aria-expanded="false" aria-label="Alternar navegación">
                                    <span class="navbar-toggler-icon"></span>
                                </button>
                            </li>
                        </ul>
                        <div id="top_menu_collapse_mobile" class="offcanvas offcanvas-end o_navbar_mobile">
                            <div class="offcanvas-header justify-content-end">
                                <button type="button" class="nav-link btn-close" data-bs-dismiss="offcanvas" aria-label="Cerrar"></button>
                            </div>
                            <div class="offcanvas-body d-flex flex-column justify-content-between h-100 w-100">
                                <ul class="navbar-nav">
                                    <li>
                                        <form method="get" class="o_searchbar_form s_searchbar_input" action="/website/search">
                                            <div role="search" class="input-group mb-3">
                                                <input type="search" name="search" class="search-query form-control oe_search_box rounded-start-pill text-bg-light ps-3 border-0 bg-light" placeholder="Buscar…"/>
                                                <button type="submit" aria-label="Búsqueda" title="Búsqueda" class="btn oe_search_button rounded-end-pill bg-o-color-3 pe-3">
                                                    <i class="oi oi-search"></i>
                                                </button>
                                            </div>
                                            <input name="order" type="hidden" value="name asc">
                                        </form>
                                    </li>
                                    <ul role="menu" class="nav navbar-nav top_menu">
                                        <li role="presentation" class="nav-item">
                                            <a role="menuitem" href="/" class="nav-link active">Inicio</a>
                                        </li>
                                        <li role="presentation" class="nav-item">
                                            <a role="menuitem" href="/contactus" class="nav-link">Contáctenos</a>
                                        </li>
                                    </ul>
                                    <li>
                                        <a href="tel:+1 555-555-5556" class="nav-link o_nav-link_secondary p-2">
                                            <i class="fa fa-1x fa-fw fa-phone me-1"></i>
                                            <span class="o_force_ltr"><small>+1 555-555-5556</small></span>
                                        </a>
                                    </li>
                                    <ul class="navbar-nav gap-2 mt-3 w-100">
                                        <li class="dropdown dropup o_no_autohide_item">
                                            <a href="#" role="button" data-bs-toggle="dropdown" class="dropdown-toggle nav-link d-flex align-items-center border-0">
                                                <i class="fa fa-1x fa-fw fa-user me-2"></i>
                                                <span class="me-auto small">Mitchell Admin</span>
                                            </a>
                                            <div role="menu" class="dropdown-menu js_usermenu w-100">
                                                <a href="/odoo" role="menuitem" class="dropdown-item ps-3">
                                                    <i class="fa fa-fw fa-th me-1 small text-primary text-primary-emphasis"></i> Aplicaciones
                                                </a>
                                                <a href="/my/home" role="menuitem" class="dropdown-item ps-3">
                                                    <i class="fa fa-fw fa-id-card-o me-1 small text-primary text-primary-emphasis"></i> Mi cuenta
                                                </a>
                                                <div class="dropdown-divider"></div>
                                                <a role="menuitem" class="dropdown-item ps-3" href="/web/session/logout?redirect=/">
                                                    <i class="fa fa-fw fa-sign-out me-1 small text-primary text-primary-emphasis"></i> Cerrar sesión
                                                </a>
                                            </div>
                                        </li>
                                        <li>
                                            <a href="/contactus" class="btn btn-primary btn_cta w-100">Contáctenos</a>
                                        </li>
                                    </ul>
                                </ul>
                            </div>
                        </div>
                    </div>
                </nav>
            </header>
            <main>
                <t t-raw="0"/>
            </main>
            <footer>
                <p>Footer of Lytics</p>
            </footer>
        </div>
    </template>

    <template id="lytics_template" name="Lytics Template">
        <t t-call="lytics_base_template">
            <div>
                <h1>Welcome to Lytics</h1>
                <p>This is a custom page for Lytics.</p>
            </div>
        </t>
    </template>
</odoo>