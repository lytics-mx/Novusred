<odoo>
    <template id="lytics_base_template" name="Lytics Base Template">
        <div class="lytics-container">
            <t t-raw="0"/>
        </div>
    </template>

    <template id="lytics_template" name="Lytics Template" inherit_id="lytics_base_template">
        <xpath expr="//div[@class='lytics-container']" position="inside">
            <h1>Welcome to Lytics</h1>
            <p>This is a custom page for Lytics.</p>
        </xpath>
    </template>
</odoo>