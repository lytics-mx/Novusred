<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template>
        <data inherit_id="website.placeholder_header_brand" name="Header Brand Logo" active="True">
            <xpath expr="//*[@id='o_fake_navbar_brand']" position="replace">
                <a data-name="Navbar Logo" href="/" t-attf-class="navbar-brand logo #{_link_class}">
                    <span t-field="website.logo" t-options="{'widget': 'image', 'width': 120, 'height': 50}" role="img" t-att-aria-label="'Logo of %s' % website.name" t-att-title="website.name"/>
                </a>
            </xpath>
        </data>
    </template>
</odoo>
